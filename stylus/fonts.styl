@font-face
  font-family 'icomoon'
  src url('fonts/icomoon.eot?m15400')
  src url('fonts/icomoon.eot?#iefixm15400') format('embedded-opentype'),
 url('fonts/icomoon.woff?m15400') format('woff'),
		url('fonts/icomoon.ttf?m15400') format('truetype'),
		url('fonts/icomoon.svg?m15400#icomoon') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'icomoon'
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-man
  &:before
    content "\e601"

.icon-woman
  &:before
    content "\e602"

.icon-warning
  &:before
    content "\e600"

